{
  "name": "rexaml",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@storybook/react": "^5.3.13",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.1.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "eslint-plugin-react": "^7.18.3",
    "install": "^0.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.1",
    "react-styleguidist": "^10.6.2",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src react-scripts start",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "test": "cross-env NODE_PATH=src react-scripts test",
    "eject": "cross-env react-scripts eject",
    "storybook": "cross-env NODE_PATH=src start-storybook -p 9003 -s public",
    "build-storybook": "cross-env NODE_PATH=src build-storybook -s public",
    "styleguide": "cross-env styleguidist server",
    "styleguide:build": "cross-env styleguidist build",
    "lint": "cross-env eslint src/components/** --ext .jsx,.js --max-warnings 0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app",
      "eslint-config-prettier",
      "plugin:jsx-a11y/strict"
    ],
    "rules": {
      "react/require-default-props": 0,
      "react/jsx-wrap-multilines": 0,
      "react/jsx-one-expression-per-line": 0,
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/preset-create-react-app": "^1.5.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "prettier": "^1.19.1"
  }
}
