{
  "name": "rexaml",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addons": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.31.11",
    "install": "^0.13.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-styleguidist": "^13.0.0",
    "styled-components": "^5.3.6"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src react-scripts start",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "test": "cross-env NODE_PATH=src react-scripts test",
    "eject": "cross-env react-scripts eject",
    "storybook": "cross-env NODE_PATH=src start-storybook -p 9003 -s public",
    "build-storybook": "cross-env NODE_PATH=src build-storybook -s public",
    "styleguide": "cross-env styleguidist server",
    "styleguide:build": "cross-env styleguidist build",
    "lint": "cross-env eslint src/components/** --ext .jsx,.js --max-warnings 0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app",
      "eslint-config-prettier",
      "plugin:jsx-a11y/strict"
    ],
    "rules": {
      "react/require-default-props": 0,
      "react/jsx-wrap-multilines": 0,
      "react/jsx-one-expression-per-line": 0,
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "prettier": "^2.8.0"
  }
}
